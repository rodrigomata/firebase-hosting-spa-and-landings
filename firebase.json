{
  "hosting": {
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/membership/*",
        "destination": "/membership/index.html"
      },
      {
        "source": "/landing/*",
        "destination": "/landing/index.html"
      },
      {
        "source": "**",
        "destination": "/public/index.html"
      }
    ]
  }
}
